{
  "name": "website-screenshot",
  "type": "module",
  "engines": {
    "node": ">=22.15.18"
  },
  "scripts": {
    "dev": "concurrently \"docker run -p 3100:3000 --rm ghcr.io/browserless/chromium:latest\" \"tsx watch src/index.ts\"",
    "dev:api": "tsx watch src/index.ts",
    "dev:browserless": "docker run -p 3100:3000 --rm ghcr.io/browserless/chromium:latest",
    "build": "tsc",
    "start": "node dist/index.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "test": "vitest",
    "test:node-screenshots": "node test/save-screenshots-node.js"
  },
  "dependencies": {
    "@browserbasehq/sdk": "^2.6.0",
    "@duckduckgo/autoconsent": "^12.20.1",
    "@extra/proxy-router": "^3.1.6",
    "@ghostery/adblocker-playwright": "^2.5.2",
    "@hono/node-server": "^1.12.0",
    "canvas": "^3.1.0",
    "cross-fetch": "^4.1.0",
    "dotenv": "^16.5.0",
    "hono": "^4.7.9",
    "idcac-playwright": "^0.1.3",
    "jimp": "^1.6.0",
    "p-queue": "^8.1.0",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "playwright": "^1.52.0",
    "playwright-core": "^1.52.0",
    "playwright-extra": "^4.3.6",
    "puppeteer-extra-plugin-recaptcha": "^3.6.8",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.0-beta.4",
    "@types/node": "^22.15.18",
    "concurrently": "^8.2.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vitest": "~3.1.3"
  }
}